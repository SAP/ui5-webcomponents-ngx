{
  "name": "documentation",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/documentation/src",
  "prefix": "ui-angular",
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "sync": {
      "command": "ts-node {projectRoot}/sync.ts",
      "outputs": ["{projectRoot}/src/api-json.json"]
    },
    "serve": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "apps/documentation/.storybook",
        "browserTarget": "documentation:build",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      },
      "dependsOn": ["sync"]
    },
    "build": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/documentation",
        "configDir": "apps/documentation/.storybook",
        "browserTarget": "documentation:build",
        "compodoc": false,
        "docs": true
      },
      "configurations": {
        "ci": {
          "quiet": true,
          "docs": true
        }
      }
    },
    "dependsOn": ["sync", "^sync", "^build"]
  },
  "tags": []
}
