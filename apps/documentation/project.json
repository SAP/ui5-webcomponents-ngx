{
  "name": "documentation",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/documentation/src",
  "prefix": "ui-angular",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/documentation/**/*.ts",
          "apps/documentation/**/*.html"
        ]
      }
    },
    "sync": {
      "executor": "@ui5/webcomponents-nx:sync",
      "options": {
        "configFiles": [
          "apps/documentation/stories-sync.conf.ts"
        ]
      },
      "outputs": [
        "apps/documentation/src/app/stories"
      ]
    },
    "serve": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "apps/documentation/.storybook",
        "browserTarget": "documentation:build",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/documentation",
        "configDir": "apps/documentation/.storybook",
        "browserTarget": "documentation:build",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "tags": []
}
