{
  "name": "fundamental-styles",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/fundamental-styles",
  "prefix": "ui-angular",
  "targets": {
    "sync": {
      "executor": "nx:run-commands",
      "options": {
        "command": "ts-node {projectRoot}/postinstall.js"
      }
    },
    "build": {
      "executor": "tools:build-partial",
      "outputs": [
        "{options.distPath}"
      ],
      "options": {
        "includedFiles": [
          "{projectRoot}/theming/**/*",
          "{projectRoot}/*.json",
          "{projectRoot}/README.md",
          "{projectRoot}/*.conf.ts",
          "{projectRoot}/postinstall.js"
        ],
        "excludedFiles": [
          "{projectRoot}/theming/supported-themes.ts",
          "{projectRoot}/tsconfig.spec.json",
          "{projectRoot}/compodocrc.json",
          "{projectRoot}/project.json"
        ],
        "distPath": "{workspaceRoot}/dist/{projectRoot}"
      },
      "configurations": {
        "production": {
          "pack": true
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  },
  "tags": []
}
