{
  "name": "fundamental-styles",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/fundamental-styles",
  "prefix": "ui-angular",
  "targets": {
    "sync": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx g @ui5/webcomponents-ngx-schematics:sync libs/fundamental-styles/wrapper.conf.ts,libs/fundamental-styles/theming.conf.ts --basePath=libs/fundamental-styles"
      },
      "outputs": [
        "{projectRoot}/directives",
        "{projectRoot}/theming/theming.config.ts",
        "{projectRoot}/fundamental-styles-components.module.ts",
        "{projectRoot}/index.ts"
      ]
    },
    "build": {
      "executor": "tools:build-partial",
      "outputs": [
        "{options.distPath}"
      ],
      "options": {
        "includedFiles": [
          "{projectRoot}/theming/**/*",
          "{projectRoot}/*.json",
          "{projectRoot}/README.md",
          "{projectRoot}/*.conf.ts"
        ],
        "excludedFiles": [
          "{projectRoot}/theming/supported-themes.ts",
          "{projectRoot}/tsconfig.spec.json",
          "{projectRoot}/compodocrc.json",
          "{projectRoot}/project.json"
        ],
        "distPath": "{workspaceRoot}/dist/{projectRoot}"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  },
  "tags": []
}
