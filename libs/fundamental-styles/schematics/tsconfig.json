{
  "extends": "../../../tsconfig.base.json",
  "compilerOptions": {
    "lib": ["es2018", "dom"],
    "declaration": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "outDir": "../../../dist/libs/ui5-angular",
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "strictNullChecks": true,
    "target": "es6",
    "types": ["node"],
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": ["**/*"],
  "exclude": ["*/files/**/*"]
}
