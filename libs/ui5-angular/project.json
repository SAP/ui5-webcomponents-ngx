{
  "name": "ui5-angular",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/ui5-angular",
  "prefix": "ui-angular",
  "targets": {
    "sync": {
      "command": "nx g @ui5/webcomponents-ngx-schematics:sync libs/ui5-angular/wrapper.conf.ts,libs/ui5-angular/theming.conf.ts --basePath=libs/ui5-angular"
    },
    "build": {
      "executor": "tools:build-partial",
      "outputs": [
        "{options.distPath}"
      ],
      "options": {
        "includedFiles": [
          "{projectRoot}/config/**/*",
          "{projectRoot}/content-density/**/*",
          "{projectRoot}/fiori/i18n/**/*",
          "{projectRoot}/icons/**/*",
          "{projectRoot}/theming/**/*",
          "{projectRoot}/*.json",
          "{projectRoot}/README.md",
          "{projectRoot}/*.conf.ts",
          "{projectRoot}/postinstall.js"
        ],
        "excludedFiles": [
          "{projectRoot}/theming/supported-themes.ts",
          "{projectRoot}/tsconfig.spec.json",
          "{projectRoot}/compodocrc.json",
          "{projectRoot}/project.json"
        ],
        "distPath": "{workspaceRoot}/dist/{projectRoot}"
      },
      "configurations": {
        "production": {
          "pack": true
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/ui5-angular/jest.config.ts"
      }
    }
  },
  "tags": []
}
