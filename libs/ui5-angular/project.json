{
  "name": "ui5-angular",
  "projectType": "library",
  "sourceRoot": "libs/ui5-angular/src",
  "prefix": "ui-angular",
  "targets": {
    "prepare": {
      "executor": "@ui5/webcomponents-nx:prepare",
      "options": {
        "configFile": "libs/ui5-angular/wrapper.conf.ts",
        "distPath": "dist/libs/ui5-angular",
        "schematics": {
          "tsConfig": "tsconfig.schematics.json",
          "collection": "schematics"
        }
      }
    },
    "sync": {
      "executor": "@ui5/webcomponents-nx:sync",
      "options": {
        "configFile": "libs/ui5-angular/wrapper.conf.ts"
      }
    },
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": [
        "dist/libs/ui5-angular"
      ],
      "options": {
        "project": "libs/ui5-angular/ng-package.json",
        "updateBuildableProjectDepsInPackageJson": false
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/ui5-angular/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/ui5-angular/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production",
      "dependsOn": [
        "sync"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/ui5-angular/**/*.ts",
          "libs/ui5-angular/**/*.html"
        ]
      }
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/ui5-angular/.storybook",
        "browserTarget": "ui5-angular:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/ui5-angular",
        "configDir": "libs/ui5-angular/.storybook",
        "browserTarget": "ui5-angular:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "tags": []
}
